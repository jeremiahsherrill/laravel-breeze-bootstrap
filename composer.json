{
    "name": "jeremiahsherrill/laravel-breeze-bootstrap",
    "description": "Minimal Laravel authentication scaffolding with Blade and Bootstrap.",
    "keywords": ["laravel", "auth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jeremiahsherrill/larave-breeze-bootstrap/issues",
        "source": "https://github.com/jeremiahsherrill/laravel-breeze-bootstrap"
    },
    "authors": [
        {
            "name": "Jeremiah Sherrill",
            "email": "jeremiahsherrill@gmail.com"
        },
        {
            "name": "Ganesh Khade",
            "email": "ganesh.khade@outlook.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/filesystem": "^9.38",
        "illuminate/support": "^9.38",
        "illuminate/validation": "^9.38"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Breeze\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Breeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
